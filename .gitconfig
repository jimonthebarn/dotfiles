[include]
	path = ~/darkroom/.gitconfig
[web]
   browser = google-chrome
[core]
	excludesFile = ~/.gitignore
    editor = subl --wait
[user]
	name = Benjamin Roth
	email = roth.benjamin@gmail.com
	useConfigOnly = true
[init]
	templatedir = ~/.git-templates
[commit]
	template = ~/.gitmessage
[credential]
	helper = osxkeychain
[url "git@github.com:"]
    # force ssh for github
	insteadOf = https://github.com/
