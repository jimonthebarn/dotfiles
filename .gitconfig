[include]
	path = ~/darkroom/.gitconfig-work
[web]
   browser = google-chrome
[core]
	excludesFile = ~/.gitignore
    #editor = sublime --project ~/dotfiles/.gitconfig.sublime-project --wait
[user]
	#name = Benjamin Roth
	#email = roth.benjamin@gmail.com
	useConfigOnly = true
[init]
	templatedir = ~/.git-templates
[commit]
	template = ~/.gitmessage
[credential]
	helper = osxkeychain
[url "git@github.com:"]
    # force ssh for github
	insteadOf = https://github.com/
